# API Keys
OPENROUTER_API_KEY=your_openrouter_api_key_here
DIRECT_API_KEY=your_direct_api_key_here

# Application Settings
AGENTIC_AI_LOG_LEVEL=INFO
AGENTIC_AI_CACHE_DIR=cache
AGENTIC_AI_ASSETS_DIR=assets
AGENTIC_AI_SITE_URL=https://agentic-ai.example.com

# Security
AGENTIC_AI_ENCRYPTION_KEY=your_encryption_key_here
AGENTIC_AI_API_KEY_ENCRYPTION=true
AGENTIC_AI_SAVE_HISTORY=true
AGENTIC_AI_ENCRYPT_HISTORY=true
AGENTIC_AI_MAX_HISTORY_ITEMS=1000

# Performance
AGENTIC_AI_MAX_TOKENS=2048
AGENTIC_AI_TEMPERATURE=0.7
AGENTIC_AI_TOP_P=0.9
AGENTIC_AI_FREQUENCY_PENALTY=0.0
AGENTIC_AI_PRESENCE_PENALTY=0.0
AGENTIC_AI_TIMEOUT=30

# Monitoring
AGENTIC_AI_ACTIVITY_TRACKING=true
AGENTIC_AI_PERFORMANCE_METRICS=true
AGENTIC_AI_API_ERRORS_THRESHOLD=5
AGENTIC_AI_RESPONSE_TIME_THRESHOLD=5000
AGENTIC_AI_MEMORY_USAGE_THRESHOLD=500

# Desktop Settings
AGENTIC_AI_STARTUP=true
AGENTIC_AI_GLOBAL_HOTKEY=Ctrl+Shift+A
AGENTIC_AI_THEME=light

# VS Code Extension
AGENTIC_AI_VSCODE_ENABLED=true
AGENTIC_AI_VSCODE_CODE_COMPLETION=true
AGENTIC_AI_VSCODE_CODE_EXPLANATION=true
AGENTIC_AI_VSCODE_REFACTORING=true

# Chrome Extension
AGENTIC_AI_CHROME_ENABLED=true
AGENTIC_AI_CHROME_PAGE_ANALYSIS=true
AGENTIC_AI_CHROME_TEXT_SELECTION=true
AGENTIC_AI_CHROME_SCREENSHOT_ANALYSIS=true

# Logging
AGENTIC_AI_LOG_FILE=logs/agentic_ai.log
AGENTIC_AI_LOG_MAX_SIZE=10485760
AGENTIC_AI_LOG_BACKUP_COUNT=5
AGENTIC_AI_LOG_TYPES=error,info,debug,warning

# Development
AGENTIC_AI_DEV_MODE=false
AGENTIC_AI_DEBUG=false
AGENTIC_AI_TEST_MODE=false 